      <AdditionalDependencies>qtmain$(LibSuffix).lib;Qt5Core$(LibSuffix).lib;Qt5Widgets$(LibSuffix).lib;Qt5Gui$(LibSuffix).lib;Qt5PrintSupport$(LibSuffix).lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="QT_DIR">
      <Value>$(QT_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="QtIncludeDir">
      <Value>$(QtIncludeDir)</Value>
    </BuildMacro>
    <BuildMacro Include="QtLibDir">
      <Value>$(QtLibDir)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>